{
	"coc.preferences.rootPatterns": [".git", ".hg", ".projections.json"],
	"coc.preferences.messageLevel": "error",
	"coc.preferences.colorSupport": true,
	"coc.preferences.maxFileSize": "10MB",
	"coc.preferences.formatOnType": true,
	"coc.preferences.formatOnSaveFiletypes": [
		"html",
		"css",
		"javascript",
		"json",
		"markdown",
		"bash",
		"php"
	],
	"coc.preferences.silentAutoupdate": true,
	"coc.preferences.bracketEnterImprove": true,
	"coc.preferences.jumpCommand": "edit",
	"coc.preferences.extensionUpdateCheck": "never",
	"coc.preferences.currentFunctionSymbolAutoUpdate": true,
	"languageserver": {
		"javascript": {
			"command": "node",
			"args": ["--lsp", "--client-id", "vim", "--client-version", "coc.nvim"],
			"filetypes": ["js", "ts", "json", "tsx"],
			"trace.server": "verbose"
		}
	},
	"prettier.trailingComma": "es5",
	"prettier.tabWidth": 2,
	"prettier.useTabs": true,
	"prettier.disableSuccessMessage": true,
	"prettier.onlyUseLocalVersion": false,
	"css.enable": true,
	"css.trace.server": "verbose",
	"html.enable": true,
	"html.trace.server": "messages",
	"html.filetypes": ["html", "handlebars", "htmldjango", "blade"],
	"html.format.unformatted": null,
	"html.format.contentUnformatted": "pre,code,textarea,p,div,form",
	"html.format.indentInnerHtml": true,
	"html.suggest.html5": true,
	"html.format.enable": true,
	"html.autoClosingTags": true,
	"json.enable": true,
	"json.trace.server": "messages",
	"json.format.enable": true,
	"tsserver.enable": true,
	"tsserver.locale": "",
	"tsserver.trace.server": "messages",
	"tsserver.npm": "/usr/bin/npm",
	"javascript.format.enabled": true,
	"javascript.showUnused": true,
	"javascript.updateImportsOnFileMove.enable": true,
	"javascript.autoClosingTags": true,
	"javascript.format.semicolons": "insert",
	"tsserver.tsdk": ".yarn/sdks/typescript/lib",
	"eslint.packageManager": "yarn",
	"eslint.nodePath": ".yarn/sdks",
	"eslint.format.enable": true
}
